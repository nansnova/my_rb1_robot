<launch>

<rosparam file="$(find my_rb1_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="my_rb1_robot" args="left_joint_position_controller right_joint_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/my_rb1_robot/joint_states"/>
</node>

</launch>